git init

git add .

git commit -m "First commit Message"

git remote add origin "GitHub-repository-URL"

git remote -v

git log

git branch

# add branch
git branch namebranch

# switch branch
git chekout namebranch

# Delete branch
git branch -d namebranch

# add branch & swich
git chekout -b name branch

# merge
git merge namebranch

# Залить
git push

# Обновить с Github
git pull

# restore commit version
git chekout commit_hesh
#example
git chekout 4ea9309bafb8ed3bc48c03db44e76377e55849b0

# вереуться в мастер после перехода в коммит
git chekout master

#Изменить коммит
git commit --amend

#Удалить коммит
git reset --hard HEAD~1
# где HEAD~1 означает спуститься на 1 позицию ниже в коммитах

# Убрать из лога комиты и остаться на последней версии
git reset --soft HEAD~3
#Где HEAD~3 это например сколько коммитовот последнего до последнего включая первый и последний
# т.е. например нажно остаться на новейшем из 3х



 
